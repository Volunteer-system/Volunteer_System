        <Button Content="取消" Name="btn_cancel" HorizontalAlignment="Left" Margin="690,516,0,0" VerticalAlignment="Top" Width="86" Height="32" BorderBrush="{x:Null}" Background="Turquoise" Foreground="White" RenderTransformOrigin="0.507,0.219" Click="btn_cancel_Click"/>
        <Label x:Name="Lab_event_no" Content="Label" HorizontalAlignment="Left" Margin="250,109,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_event_name" Content="Label" HorizontalAlignment="Left" Margin="250,141,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_event_type" Content="Label" HorizontalAlignment="Left" Margin="250,175,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_volunteer" Content="Label" HorizontalAlignment="Left" Margin="250,211,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_application_unit" Content="Label" HorizontalAlignment="Left" Margin="250,249,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_stage" Content="Label" HorizontalAlignment="Left" Margin="250,285,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Lab_notification_date" Content="Label" HorizontalAlignment="Left" Margin="250,319,0,0" VerticalAlignment="Top"/>
        <TextBox Name="tb_Volunteer" FontSize="16" HorizontalAlignment="Left" Height="100" Margin="499,106,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="363" ScrollViewer.VerticalScrollBarVisibility="Auto" ScrollViewer.HorizontalScrollBarVisibility="Disabled"/>
        <Label Content="志工自述：" HorizontalAlignment="Left" Margin="499,76,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox Name="tb_Supervisor" FontSize="16" HorizontalAlignment="Left" Height="100" Margin="499,248,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="363"/>
        <Label Content="督導敘述：" HorizontalAlignment="Left" Margin="499,218,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox Name="tb_result" FontSize="16" HorizontalAlignment="Left" Height="100" Margin="499,390,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="363"/>
        <Label Content="處理結果：" HorizontalAlignment="Left" Margin="499,360,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label x:Name="Lab_closing_date" Content="Label" HorizontalAlignment="Left" Margin="250,355,0,0" VerticalAlignment="Top"/>
        <Label Content="必填，不可空白" Foreground="Red" HorizontalAlignment="Left" Margin="581,80,0,0" VerticalAlignment="Top"/>
        <Label Content="必填，不可空白" Foreground="Red" HorizontalAlignment="Left" Margin="581,222,0,0" VerticalAlignment="Top"/>
        <Label Content="必填，不可空白" Foreground="Red" HorizontalAlignment="Left" Margin="581,364,0,0" VerticalAlignment="Top"/>

