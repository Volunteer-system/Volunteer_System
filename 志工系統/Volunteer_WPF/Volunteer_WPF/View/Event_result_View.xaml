<Window x:Class="Volunteer_WPF.View.Event_result"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Volunteer_WPF.View"
        mc:Ignorable="d"
        Title="Event_result" Height="656.5" Width="995">
    <Grid Margin="0,0,0,-7">
        <Rectangle HorizontalAlignment="Left" Height="567" Margin="33,30,0,0" Stroke="#FFA7E6DA" VerticalAlignment="Top" Width="918" StrokeThickness="5" RadiusX="20" RadiusY="20"/>

