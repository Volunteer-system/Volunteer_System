<Window x:Class="Volunteer_WPF.View.Shift_schedule_View"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Volunteer_WPF.View"
        mc:Ignorable="d"
        Title="Shift_schedule_View" Height="615" Width="1020">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="51*"/>
            <ColumnDefinition Width="287*"/>
        </Grid.ColumnDefinitions>
        <DockPanel x:Name="Dock_Frame" LastChildFill="True" Margin="1,1,0.6,0.6">
            <StackPanel  DockPanel.Dock="Left"  Width="150"  Margin="1" HorizontalAlignment="Left">
                <Label  Content="運用單位清單" FontSize="14"/>
                <ScrollViewer  DockPanel.Dock="Left"  Width="150"   HorizontalAlignment="Left">
                    <ListBox x:Name="list_units" Height="582" Width="150">
                    </ListBox>
                </ScrollViewer>
            </StackPanel>            
        </DockPanel>
        <TabControl x:Name="TabControl1" Grid.Column="1">            
        </TabControl>
    </Grid>
</Window>
